  "info": {
    "title": "PRT VideoDownloader API",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/downloads/initiate": {
      "post": {
        "operationId": "initiate_download_downloads_initiate_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InitiateDownloadRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InitiateDownloadResponse"
                }
              }
            },
            "description": "OK"
          },
          "418": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "I'm a Teapot"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ValidationErrorModel"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Unprocessable Content"
          }
        },
        "summary": "Initiate a video download",
        "tags": [
          "downloads"
        ]
      }
    },
    "/downloads/list": {
      "get": {
        "operationId": "list_downloads_downloads_list_get",
        "parameters": [
          {
            "description": "Time window in minutes to include downloads from now back to this many minutes",
            "in": "query",
            "name": "overLastPeriodDurationInMinutes",
            "required": false,
            "schema": {
              "default": 60,
              "description": "Time window in minutes to include downloads from now back to this many minutes",
              "minimum": 1.0,
              "title": "Overlastperioddurationinminutes",
              "type": "integer"
            }
          },
          {
            "description": "If true, only include downloads that are currently ongoing (PENDING or DOWNLOADING)",
            "in": "query",
            "name": "onlyShowOngoingDownloads",
            "required": false,
            "schema": {
              "default": false,
              "description": "If true, only include downloads that are currently ongoing (PENDING or DOWNLOADING)",
              "title": "Onlyshowongoingdownloads",
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ListDownloadsResponse"
                }
              }
            },
            "description": "OK"
          },
          "418": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "I'm a Teapot"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ValidationErrorModel"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Unprocessable Content"
          }
        },
        "summary": "List downloads",
        "tags": [
          "downloads"
        ]
      }
    },
    "/downloads/publicURL": {
      "get": {
        "operationId": "get_public_url_downloads_publicURL_get",
        "parameters": [
          {
            "description": "UUID of the download request",
            "in": "query",
            "name": "videoDownloadUUID",
            "required": true,
            "schema": {
              "description": "UUID of the download request",
              "title": "Videodownloaduuid",
              "type": "string"
            }
          },
          {
            "description": "Type of URL to generate: HTTPS or S3",
            "in": "query",
            "name": "urlType",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/URLTypeEnum",
              "description": "Type of URL to generate: HTTPS or S3"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetPublicURLResponse"
                }
              }
            },
            "description": "OK"
          },
          "418": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "I'm a Teapot"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ValidationErrorModel"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Unprocessable Content"
          }
        },
        "summary": "Get public URL for a downloaded video",
        "tags": [
          "downloads"
        ]
      }
